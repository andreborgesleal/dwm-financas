@model System.Web.Mvc.PagedList<DWM.Models.Repositories.PlanoContaViewModel>
@{
    Layout = null;
    Model.action = "../PlanoContas/_ListPlanoContaModal";
}

<div class="row">
    <div class="col-md-12" style="margin: 0px">
        @Html.Partial("_Filter", Model)
        <div id="div-pc" style="padding-top: 0px">
            @Html.Partial("../PlanoContas/_ListPlanoContaModal", Model)
        </div>
    </div>
</div>

<script type="text/javascript">

    function Move(id, descricao, codigoPleno) {
        $("#planoContaId").val(id);
        $("#codigoPleno").val(codigoPleno);
        $("#descricao_conta").val(descricao);
        $("#descricao_conta1").val(descricao);
        $("#btnModal").click();
    }

</script>










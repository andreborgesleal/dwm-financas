@using App_Dominio.Repositories
@model PagedList<DWM.Models.Repositories.CobrancaViewModel>
@{
    TransacaoRepository[] t = { new TransacaoRepository(), new TransacaoRepository() };
    t[0].url = "../CobrancaCad/Create";
    t[0].nomeCurto = "Incluir";
    t[0].referencia = "";

    t[1].url = "../Home/Default";
}
@Html.Partial("_alerts")
<div class="x_panel">
    @Html.EditorFor(info => Model, "FORMULARIO", new { Brand = "Cadastro de Cobrança", Descricao = "Listagem do cadastro de cobrança", Transacao = t })
    <div class="x_content">
        <div id="div-list">
            @Html.Partial("List", Model)
        </div>
        <div class="altura-35">
            @Html.Partial("_defaultListButtons", new List<ButtonRepository> { new ButtonRepository() { linkText = "Incluir", actionName = "Create", controllerName = "CobrancaCad", buttonType = "link" } })
        </div>
    </div>
</div>

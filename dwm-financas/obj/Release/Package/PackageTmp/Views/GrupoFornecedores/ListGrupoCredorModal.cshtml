@model System.Web.Mvc.PagedList<DWM.Models.Repositories.GrupoCredorViewModel>
@{
    Layout = null;
    Model.action = "../GrupoFornecedores/_ListGrupoCredorModal";
}
<div class="row">
    <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12" style="margin: 0px">
        @Html.Partial("_Filter", Model)
        <div id="div-gcre" class="no-padding">
            @Html.Partial("../GrupoFornecedores/_ListGrupoCredorModal", Model)
        </div>
    </div>
</div>
<script type="text/javascript">

    function Move(id, descricao) {
        $("#grupoCredorId").val(id);
        $("#nome_grupoFornecedor").val(descricao);
        $("#nome_grupoFornecedor1").val(descricao);
        $("#btnModal").click();
    }

</script>




@model System.Web.Mvc.PagedList<DWM.Models.Repositories.HistoricoViewModel>
@{
    Layout = null;
    Model.action = "../Historicos/_ListHistoricoContabilidadeModal";
}
<p><h4>Históricos</h4></p>
<hr />
<div id="div-his">
    @Html.Partial("../Historicos/_ListHistoricoModal", Model)
</div>

<script type="text/javascript">

    function Move(id, descricao) {
        $("#historicoId").val(id);
        $("#descricao_historico").val(descricao);
        $("#descricao_historico1").val(descricao);
        $("#btnModal").click();
    }

</script>


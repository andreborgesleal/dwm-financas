@model System.Web.Mvc.PagedList<DWM.Models.Repositories.EnquadramentoViewModel>
@{
    Layout = null;
    Model.action = "../Enquadramentos/_ListEnquadramentoOperacaoModal";
}
<div class="row">
    <div class="col-md-12" style="margin: 0px">
        @Html.Partial("_Filter", Model)
        <div id="div-enq" class="no-padding">
            @Html.Partial("../Enquadramentos/_ListEnquadramentoOperacaoModal", Model)
        </div>
    </div>
</div>

<script type="text/javascript">

    function Move(id, descricao) {
        $("#enquadramentoId").val(id);
        $("#descricao_enquadramento").val(descricao);
        $("#descricao_enquadramento1").val(descricao);
        $("#btnModal").click();
    }

</script>


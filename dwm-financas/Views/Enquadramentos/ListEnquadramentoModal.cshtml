@model System.Web.Mvc.PagedList<DWM.Models.Repositories.EnquadramentoViewModel>
@{
    Layout = null;
    Model.action = "../Enquadramentos/_ListEnquadramentoModal";
}

<div id="div-enq">
    @Html.Partial("../Enquadramentos/_ListEnquadramentoModal", Model)
</div>

<script type="text/javascript">

    function Move(id, descricao) {
        var url = 'Enquadramento';
        url = encodeURI(url + '?enquadramentoId=' + id);
        $('#div-item').load(url);
        $("#btnModal").click();
    }

</script>




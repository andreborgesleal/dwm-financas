@model System.Web.Mvc.PagedList<DWM.Models.Repositories.EnquadramentoViewModel>
@using DWM.Models.Repositories;
@{
    Layout = null;
}
@if (Model != null)
{
    <table class="table table-striped table-condensed dedo-indicador margem-negativa">
        <thead class="corvo text-white">
            <tr>
                <th class="text-center">Ação</th>
                <th>Descrição</th>
            </tr>
        </thead>
        @foreach (EnquadramentoViewModel enq in Model)
        {
            <tr>
                <td class="col-md-2 text-center">
                    <button type="button" class="btn btn-info btn-sm glyphicon glyphicon-edit" onclick="window.location='Edit?enquadramentoId=@enq.enquadramentoId'"></button>
                    <button type="button" class="btn btn-danger btn-sm glyphicon glyphicon-trash" onclick="window.location='Delete?enquadramentoId=@enq.enquadramentoId'"></button>
                </td>
                <td>
                    <small>@enq.descricao</small>
                </td>
            </tr>
        }
    </table>
    @Html.Partial("Paginacao", Model)
}
else
{
    @Html.Partial("_AcessoNegadoModal")
}



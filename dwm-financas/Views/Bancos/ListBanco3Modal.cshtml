@model System.Web.Mvc.PagedList<DWM.Models.Repositories.BancoViewModel>
@{
    Layout = null;
    Model.action = "../Bancos/_ListBanco3Modal";
}
@Html.EditorFor(info => Model, "NAVBAR", new { brand = "Bancos" })
<div class="container-fluid">
    <div id="div-ban3">
        @Html.Partial("../Bancos/_ListBanco3Modal", Model)
    </div>
</div>

<script type="text/javascript">

    function Move(id, descricao) {
        $("#banco3Id").val(id);
        $("#nome_banco3").val(descricao);
        $("#nome_banco31").val(descricao);
        $("#btnModal").click();
    }

</script>






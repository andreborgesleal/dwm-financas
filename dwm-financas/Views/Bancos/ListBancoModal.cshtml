@model System.Web.Mvc.PagedList<DWM.Models.Repositories.BancoViewModel>
@{
    Layout = null;
    Model.action = "../Bancos/_ListBancoModal";
}
<div class="row">
    <div class="col-md-12" style="margin: 0px">
        @Html.Partial("_Filter", Model)
        <div id="div-ban" class="no-padding">
            @Html.Partial("../Bancos/_ListBancoModal", Model)
        </div>
    </div>
</div>
<script type="text/javascript">

    function Move(id, descricao) {
        $("#bancoId").val(id);
        $("#nome_banco").val(descricao);
        $("#nome_banco1").val(descricao);
        $("#btnModal").click();
    }

</script>




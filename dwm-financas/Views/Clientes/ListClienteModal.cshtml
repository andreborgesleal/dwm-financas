@model System.Web.Mvc.PagedList<DWM.Models.Repositories.ClienteViewModel>
@{
    Layout = null;
    Model.action = "../Clientes/_ListClienteModal";
}

<div class="row">
    <div class="col-md-12" style="margin: 0px">
        @Html.Partial("_Filter", Model)
        <div id="div-cli" class="no-padding">
            @Html.Partial("../Clientes/_ListClienteModal", Model)
        </div>
    </div>
</div>

<script type="text/javascript">

    function Move(id, descricao) {
        $("#clienteId").val(id);
        $("#nome_cliente").val(descricao);
        $("#nome_cliente1").val(descricao);
        $("#btnModal").click();
    }

</script>





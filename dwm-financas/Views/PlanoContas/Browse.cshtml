@using App_Dominio.Repositories
@model PagedList<DWM.Models.Repositories.PlanoContaViewModel>
@{
    TransacaoRepository[] t = { new TransacaoRepository(), new TransacaoRepository() };
    t[0].url = "../PlanoContas/Create";
    t[0].nomeCurto = "Incluir";
    t[0].referencia = "";
    t[0].exibir = "S";

    t[1].url = "../Home/Default";
}
<!-- Datatables -->
<link href="../Content/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">

<div class="container-cadastro">
    @Html.Partial("_alerts")
    <div class="x_panel">
        @Html.EditorFor(info => Model, "FORMULARIO", new { Brand = "Plano de Contas", Descricao = "Listagem do Plano de Contas", Transacao = t })
        <div class="x_content">
            <div id="div-list">
                @Html.Partial("List", Model)
            </div>
            <div class="altura-35">
                @Html.Partial("_defaultListButtons", new List<ButtonRepository> { new ButtonRepository() { linkText = "Incluir", actionName = "Create", controllerName = "PlanoContas", buttonType = "link" } })
            </div>
        </div>
    </div>
</div>

